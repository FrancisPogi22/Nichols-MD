<link rel="preload" href="{{ 'about.css' | asset_url }}" as="style" onload="this.onload=null;this.rel='stylesheet'">
<noscript>{{ 'about.css' | asset_url | stylesheet_tag }}</noscript>
<link rel="preload" href="{{ 'philosophy.css' | asset_url }}" as="style" onload="this.onload=null;this.rel='stylesheet'">
