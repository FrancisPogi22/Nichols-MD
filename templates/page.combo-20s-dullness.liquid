          <defs>
            <path
              id="circle"
              d="M 100, 100
                    m -75, 0
                    a 75, 75 0 1, 0 150, 0
                    a 75, 75 0 1, 0 -150, 0
                    "
            ></path>
          </defs>
          <text width="200">
            <textPath
              href="#circle"
              class="text"
              startOffset="50%"
              text-anchor="middle"
              dominant-baseline="middle"
            >
              You came to the right place
            </textPath>
