        <svg
          class="rotatingText"
          viewBox="0 0 200 200"
          width="200"
          height="200"
        >
          <circle cx="100" cy="100" r="12" fill="white" stroke-width="2" />
          <defs>
            <path
              id="circle"
              d="M 100, 100
                    m -75, 0
                    a 75, 75 0 1, 0 150, 0
                    a 75, 75 0 1, 0 -150, 0
                    "
            ></path>
          </defs>
          <text width="200">
            <textPath
              href="#circle"
              class="text"
              startOffset="50%"
              text-anchor="middle"
              dominant-baseline="middle"
            >
              You came to the right place
            </textPath>
          </text>
