<script>
  $(document).ready(function () {
    $('.faqs__header').click(function () {
      $('.faqs__content').slideUp(200);
      $('.faqs__widget').removeClass('active');
      $('.faqs__header svg').css('transform', 'rotate(0deg)');

      if (!$(this).next('.faqs__content').is(':visible')) {
        $(this).parent('.faqs__widget').addClass('active');
        $(this).next('.faqs__content').slideDown(200);
        $(this).find('svg').css('transform', 'rotate(180deg)');
      }
    });
  });
</script>
